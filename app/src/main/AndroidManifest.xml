<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.lsukev.lsufootball">

    <application
        android:allowBackup="true"
        android:icon="@drawable/lsutigerface"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activities.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activities.RosterListActivity" />
        <activity android:name=".activities.BioActivity" />
        <activity android:name=".activities.BaseActivity" />
        <activity android:name=".activities.ScheduleActivity" />
        <activity android:name=".activities.RosterCardActivity" />
        <activity android:name=".activities.NotificationTestActivity" />
        <activity android:name=".activities.TestActivity" />
    </application>

</manifest>